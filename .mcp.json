{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": ["@playwright/mcp@latest"]
    },
    "serena": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant"
      ]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
    },
    "mysql": {
      "command": "npx",
      "args": [
        "-y",
        "@fhuang/mcp-mysql-server"
      ],
      "env": {
        "MYSQL_HOST": "IP",
        "MYSQL_USER": "USER",
        "MYSQL_PORT": "PORT",
        "MYSQL_PASSWORD": "password",
        "MYSQL_DATABASE": "DATABASE"
      }
    }
  }
}
